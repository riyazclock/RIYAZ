<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ControllerServlet</servlet-name>
  	<display-name>ControllerServlet</display-name>
  	<description></description>
  	<servlet-class>com.Management.ControllerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Controller</servlet-name>
  	<display-name>Controller</display-name>
  	<description></description>
  	<servlet-class>com.management.Controller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Compliance</servlet-name>
  	<display-name>Compliance</display-name>
  	<description></description>
  	<servlet-class>com.model.Compliance</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ControllerEmployeeServlet</servlet-name>
  	<display-name>ControllerEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.controller.ControllerEmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControllerServlet</servlet-name>
  	<url-pattern>/ControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Controller</servlet-name>
  	<url-pattern>/Controller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Compliance</servlet-name>
  	<url-pattern>/Compliance</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControllerEmployeeServlet</servlet-name>
  	<url-pattern>/ControllerEmployeeServlet</url-pattern>
  </servlet-mapping>
</web-app>
